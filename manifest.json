{
  "manifest_version":2,
  "name":"interactable",
  "version":"1.0.1",
  "description":"Checks selectors on a site to see if interactable elements contain a certain selector.",
  "icons":{
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
  "browser_action":{
    "default_icon":"icon16.png"
  },
  "options_ui":{
    "page":"options.html",
    "open_in_tab":false
  },
  "permissions":[
    "tabs",
    "storage",
    "*://*/*"
  ],
  "background":{
    "scripts":[
      "background.js"
    ],
    "persistent":false
  }
}